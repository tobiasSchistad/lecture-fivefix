  },
  "homepage": "https://github.com/tobiasSchistad/lecture-fivefix#readme",
  "devDependencies": {
    "husky": "^7.0.4",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "Husky": "npm:husky@^7.0.4"
  }
